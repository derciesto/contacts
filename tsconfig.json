{
  "compileOnSave": true,
  "compilerOptions": {
    "allowUnreachableCode": false,
    "declaration": true,
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "importHelpers": false,
    "outDir": ".",
    "module": "commonjs",
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "pretty": true,
    "strict": true,
    "target": "es5",
    "lib": [
      "es2015",
      "dom"
    ]
  },
  "exclude": [],
  "include": ["lib/contacts.ts", "lib/index.ts"]
}
